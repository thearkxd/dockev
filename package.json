{
  "name": "dockev",
  "version": "0.1.0",
  "description": "Your personal command center for all development projects",
  "author": "thearkxd",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "electron/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/thearkxd/dockev.git"
  },
  "bugs": {
    "url": "https://github.com/thearkxd/dockev/issues"
  },
  "homepage": "https://github.com/thearkxd/dockev#readme",
  "keywords": [
    "electron",
    "project-manager",
    "developer-tools",
    "ide-launcher",
    "git-integration",
    "productivity",
    "dockev",
    "project management",
    "project tracking",
    "project organization"
  ],
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "tsc -b && vite build",
    "build:electron": "npm run build && electron-builder",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "electron": "^39.2.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@iconify/react": "^6.0.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^8.0.1"
  }
